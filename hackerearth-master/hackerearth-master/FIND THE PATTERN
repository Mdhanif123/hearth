#include<iostream>
using namespace std;
int main()
{
	long long int n,i;
	cin>>n;
	long long int a[n];
	for(i=0;i<n;i++)
	cin>>a[i];
	long long int min=a[0],max=a[0];
	for(i=0;i<n;i++)
	{
		if(a[i]<min)
		min=a[i];
	}
	for(i=0;i<n;i++)
	{
		if(a[i]>max)
		max=a[i];
	}
	long long int result = min+max;
	cout<<result;
}
